package chat.usergroup;

import java.lang.String;

/*
 * This interface is used to edit/retrieve the information of
 * user group a chat system maintains.
 * It stores user information <name, ipaddress, port>
 * 
 */
public interface UserGroup {
	
	public boolean add(String name, String ip_port);
	public String toString();
	
}
